#[path = "../src/ds/mod.rs"]
mod ds;

#[cfg(test)]
mod tests {
    use chrono::{Utc};

    use crate::ds::{ds_point::DsPoint, ds_status::DsStatus};

    #[test]
    fn construct() {
        let value = true;
        let point = DsPoint::<bool>::new(
            "test.point",
            value,
            DsStatus::Ok,
            0,
            0,
            Utc::now(),
        );
        assert_eq!(point.value, true);
    }
}